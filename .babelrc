{
    "presets": ["module:metro-react-native-babel-preset"],
    "plugins": [
      [
        "module-resolver",
        {
          "root": ["./"],
          "extensions": [
            ".ios.ts",
            ".android.ts",
            ".ts",
            ".ios.tsx",
            ".android.tsx",
            ".tsx",
            ".jsx",
            ".js",
            ".json"
          ],
          "alias": {
            "@components": "./UI/components",
            "@core": "./UI/core",
            "@screens": "./screens",
            "@routes": "./routes",
            "@store": "./redux"
          }
        }
      ]
    ]
  }